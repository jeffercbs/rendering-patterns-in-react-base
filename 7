import {RC} from "react"
type ChildComponentProps = {
  render: (name: string) => JSX.Element
}

export const ChildComponent: FC<ChildComponent> = ({render})
  const name = "Jeffercbs"
  
  return (
    <>
      {render(name)}
    </>
  );
};

export const ParentComponent = () => {
  const renderChild = (name) => <h1>Hello, {name}</h1>
  return (
    <>
    <ChildComponentProps render={renderChild} />
    </>
  );
};
